algoritmo "somadorNumerico"

var
cont,N,soma,maior:Inteiro
inicio
cont <- 1
soma <- 0
Enquanto (cont<=10)faca
   Escreva("Digite um valor:")
   Leia(N)
   Se (N > maior)entao
      maior <-N
   FimSe
   cont <- cont + 1
   soma <- N + soma
FimEnquanto
Escreval("A soma vai ser de ",soma)
Escreva("O numero maior e:",maior)

fimalgoritmo



