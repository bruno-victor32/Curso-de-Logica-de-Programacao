algoritmo "Contador"

var
valor_inicial,valor_final,contador:Inteiro
inicio
Escreval("Você quer iniciar a contagem de que valor?")
Leia(valor_inicial)
Escreval("Você quer contar até quanto?")
Leia(valor_final)
contador <- valor_inicial
Enquanto (contador  >= valor_final)faca
    Escreval("A contagem está",contador)
   contador <- contador -1
FimEnquanto
fimalgoritmo
